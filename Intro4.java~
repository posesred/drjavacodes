public class Intro4{
  public static void main(String[] args){
  System.out.println("H\n E\n  L\n   L\n    O\n   L\n  L\n E\nH");
  System.out.println(5 + " x " + 16 + " = " + (5 *16));
  System.out.println(3+ " - " + 4 + " = " + (3-4));
  System.out.println(3 + " / " + 4 + " = " + (3.0/4));  
  
  }
// division in java follws a weird rule ad int / int = int
}