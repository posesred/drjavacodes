import java.util.*;
public class Conditional4{
  public static void main(String[] args){
  //Declarations
  Scanner x = new Scanner(System.in);
  System.out.println("What is the weeather? Enter in frenheight:" );
  int temp = x.nextInt();
  if (temp>120){
  System.out.println(" are you swimming in lava " );
  }
  
  else if(temp> 90){
  System.out.println(" it’s hot " );
  }
  
  else if(temp>80){
  System.out.println("it’s shorts weather " );
  }
  
  else if(temp<45){
  System.out.println("it sweater weather " ); 
  }
   // goes in order 
    // 
  
  else if (temp<30){
  System.out.println(" it’s cold" );
  }
  
  else if  (temp< 20){
  System.out.println(" you are an ice cube " );
  }
  
  else{System.out.println("it’s beautiful outside" );  
  }
  
  // else if is used to connectt ifs together 
  }
}